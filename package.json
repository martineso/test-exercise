{
  "name": "test-exercise",
  "version": "0.0.1",
  "author": "AlexG",
  "license": "MIT",
  "scripts": {
    "test": "jest --no-cache"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".*\\.spec.js$",
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest",
        {
          "diagnostics": false
        }
      ]
    }
  }
}
